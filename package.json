{
  "name": "appsync-chat-test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "aws-amplify": "^1.1.17",
    "aws-amplify-react": "^2.2.4",
    "aws-appsync": "^1.7.0",
    "aws-appsync-react": "^1.2.5",
    "bootstrap": "^4.1.3",
    "graphql-tag": "^2.10.0",
    "node-sass-chokidar": "^1.3.4",
    "react": "^16.7.0",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "reactstrap": "^6.5.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "yarn run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint src/**/*.js src/**/*.jsx",
    "precommit": "yarn run lint",
    "prepush": "yarn run lint"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "9.0.0",
    "babel-runtime": "^6.26.0",
    "eslint": "5.6.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.3.0",
    "npm-run-all": "^4.1.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
